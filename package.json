{
    "name": "vscode-hla",
    "displayName": "vscode-hla",
    "description": "",
    "version": "0.0.1",
    "publisher": "hiro",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "hla",
            "aliases": ["hla", "hla"],
            "extensions": [".hla", ".hhf"],
