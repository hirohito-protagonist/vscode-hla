        }],
        "grammars": [{
            "language": "hla",
            "scopeName": "source.hla",
            "path": "./syntaxes/hla.tmLanguage.json"
        }],
        "snippets": [
            {
              "language": "hla",
              "path": "./snippets/hla.json"
            }
        ]
    }
}